.article-content {
  width: 750rpx;
  height: 600rpx;
  margin-top: 184rpx;
}

.no-result-icon {
  width: 420rpx;
  height: 350rpx;
  padding: 175rpx;
}
/* 隐藏滚动条 */
::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}


