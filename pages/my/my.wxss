.container{
  font-size: 0
}
.none{
  display: none
}
.mylabel {
  width: 750rpx;
  height: 88rpx;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
  box-sizing: border-box;
  position: relative;
  background: #fff;
}
.mylabel .title {
  position: absolute;
  left: 100rpx;
  font-size: 32rpx;
  color: #000000;
  letter-spacing: 0;
}
.mylabel .more {
  position: absolute;
  font-size:28rpx;
  color:#999;
  right: 50rpx;
}
.mylabel .arrow {
  position: absolute;
  height: 26rpx;
  width: 14rpx;
  right: 20rpx;
}
.mylabel .my_icon {
  width: 42rpx;
  height: 42rpx;
}
.userinfo {
  position: relative;
  width: 750rpx;
  height: 266rpx;
}
.userinfo .avatar-wrapper {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  width: 750rpx;
  position: relative;
}
.avatar-wrapper .avatar {
  position: absolute;
  width: 132rpx;
  height: 132rpx;
  border-radius: 50%;
  border: 25rpx solid white;
}
.avatar-wrapper .name {
  position: absolute;
  margin-top: 100rpx;
  font-size: 30rpx;
  color: #333;
}
.section{
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-bottom: 10rpx
}
.item_list{
  width: 210rpx;
  height: 168rpx;
  margin:10rpx 10rpx;
  display: inline-block;
}
.item_book_img{
  width: 210rpx;
  height: 118rpx;
  border-radius: 10rpx
}
.item_book_text{
  font-size: 28rpx;
  text-align: left;
  color: #333;
  height: 40rpx;
  line-height: 40rpx;
  width: 210rpx;
  margin-top: 5rpx
}
.inaline{
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis
}

