.vip-item-details {
  margin-left: 30rpx;
  width: 480rpx;
}
.vip-item-details>.name {
  line-height: 44rpx;
  color: #333333;
  font-size: 32rpx;
  font-weight: bold;
}
.detail-icon {
  height: 28rpx;
}
.vip-item-details>.desc {
  line-height: 34rpx;
  color: #999999;
  font-size: 24rpx;
  width: 480rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.vip-item-prices {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  color: #FF6246;
}
.vip-item-prices>.price {
  height: 56rpx;
  line-height: 56rpx;
  font-size: 40rpx;
  font-weight: bold;
}
.vip-item-prices>.price>.dollar {
  font-size: 24rpx;
}
.vip-item-prices>.oldprice {
  color: #999999;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 24rpx;
  text-decoration: underline;
}
/* 权益页 */
.benefits {
  width: 690rpx;
  margin: 30rpx 30rpx 170rpx;
}
.benefits>.imginfo {
  width: 100%;
  height: 530rpx;
}
.benefits>.imginfo.onelayer {
  height: 360rpx;
}

/* 悬浮在底部的支付区 */
.pay {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  background: #FFF;
  display: flex;
}
.pay-summary {
  width: 500rpx;
  padding-left: 36rpx;
}
.pay-summary-all {
  height: 50rpx;
  line-height: 50rpx;
  color: #333333;
  font-size: 36rpx;
  margin: 16rpx 0 4rpx;
}
.pay-summary-all>.info {
  color: #FF6246;
}
.pay-summary-all>.info:nth-child(2) {
  color: #999999;
  font-size: 28rpx;
}
.pay-summary-info {
  height: 28rpx;
  line-height: 28rpx;
  font-size: 20rpx;
  color: #CCCCCC;
}
.pay-summary-info>.info {
  color: #999999;
  text-decoration: underline;  
}
.pay-summary-info>.info:first-of-type {
  margin: 0 10rpx 0 -4rpx;
}

.pay-btn {
  position: absolute;
  right: 0;
  width: 240rpx;
  height: 100%;
  background: #FF6246;
  color: #FFF;
  font-size: 36rpx;
  font-weight: bold;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.pay-btn-save {
  font-size: 24rpx;
  font-weight: normal;
}

/* 支付成功页 */
.flex-wrapper {
  /* background: #ECECEC; */
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #333333;
}
.pay-icon {
  margin: 40rpx 0 10rpx;
  width: 180rpx;
  height: 180rpx;
}
.pay-title {
  height: 56rpx;
  line-height: 56rpx;
  font-weight: bold;
  font-size: 40rpx;
}
.payfail-desc {
  margin: 4rpx 0 50rpx;
  height: 36rpx;
  line-height: 36rpx;
  color: #666666;
  font-size: 26rpx;
}
.pay-goods {
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding: 0 40rpx;
}
.pay-goods[class*="smallfont"] {
  line-height: 44rpx;
  font-size: 28rpx;
}
.paysuccess>.pay-goods.smallfont1 {
    margin: 60rpx 0 22rpx;
}
.paysuccess>.pay-goods.smallfont2 {
    margin-bottom: 60rpx;
}
.pay-goods.smallfont1>.pay-valid {
  text-align: right;
}
.pay-goods[class*="largefont"] {
  line-height: 120rpx;
  font-size: 32rpx;
  color: #666666;
  border-bottom: 2rpx #ECECEC solid;
}
.pay-goods.largefont1>.pay-valid {
  color: #FF6246;
  font-size: 36rpx;
  font-weight: bold;
}
.pay-goods.largefont2>.pay-valid {
  color: #333333;
  font-weight: bold;
}
.payfail>.pay-goods.largefont2 {
    margin-bottom: 30rpx;
}
.payfail>.pay-goods[class*="smallfont"] {
  line-height: 40rpx;
}
.pay-name {
  width: 50%;
  font-weight: bold;
}
.payfail>.pay-goods>.pay-name {
  width: 30%;
  font-weight: normal;
}
.paysuccess-prize {
  padding: 20rpx 0;
  width: 100%;
  height: 344rpx;
  background: #F8F8F8;
  box-sizing: content-box;
}
.paysuccess-gift {
  width: 100%;
  height: 344rpx;
  background: #FFF;
}
/* 支付失败页 */

.btn {
  width: 670rpx;
  height: 94rpx;
  margin: 60rpx 40rpx;
  border-radius: 44rpx;
  background: #FFD71C;
  line-height: 94rpx;
  text-align: center;
  font-size: 36rpx;
  color: #333333;
}

/* 腾讯源qq 微信确认授权弹窗 */
.tencent-auth-wrapper {  
  position: fixed;
  width: 750rpx;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 200;
  background-color: rgba(0,0,0,0.5);
  display: flex;
  justify-content: center;
  align-items: center;
}
.tencent-auth-toast {
  position: relative;
  width: 560rpx;
  height: 432rpx;
  background-color: white;
  border-radius: 24rpx;
  z-index: 201;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  padding: 50rpx 30rpx;
}
.tencent-auth-toast>.quit {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}
.tencent-auth-toast>.title {
  color: #030303;
  font-size: 34rpx;
  line-height: 48rpx;
}
.tencent-auth-toast>.tips {
  color: #030303;
  font-size: 26rpx;
  height: 64rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-bottom: 40rpx;
}
.detail-zone {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.detail-zone>.detail-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 176rpx;
  margin: 0 50rpx;
}
.detail-item>.avatar {
  width: 88rpx;
  height: 88rpx;
  margin-bottom: 10rpx;
}
.detail-item>.name {
  font-size: 24rpx;
  color: #333333;
  line-height: 34rpx;
}
.detail-item>.valid {
  font-size: 20rpx;
  color: #666666;
  line-height: 28rpx;
}
