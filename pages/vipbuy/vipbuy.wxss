
.wrapper {
  background: #ECECEC;
  position: absolute;
}

.list {
  width: 690rpx;
  margin: 50rpx 30rpx 0;
  padding: 30rpx 0;
  border-radius: 20rpx;
  background: #FFF;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.userinfo {
  height: 56rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding: 0 30rpx;
}
.acct {
  position: absolute;
  left: 60rpx;
  font-weight: bold;
  color: #333333;
  font-size: 32rpx;
}
.avatar {
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  margin-right: 16rpx;
}
.userinfo>.name {
  color: #333333;
  font-size: 28rpx;
  width: 120rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.hr {
  width: 630rpx;
  height: 2rpx;
  background: #ECECEC;
  margin: 30rpx 0;
}
.vip-title {
  width: 100%;
  padding-left: 30rpx;
  display: flex;
  align-items: center;
}
.vstrip {
  width: 8rpx;
  height: 36rpx;
  background: #FCB232;
  margin-right: 10rpx;
}
.vip-type {
  font-size: 36rpx;
  color: #333333;
  margin-right: 10rpx;
}
.vip-desc {
  color: #FCB232;
  font-size: 28rpx;
}
/* 产品包列表 */
