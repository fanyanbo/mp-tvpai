.container{text-align: center;}
.tabParents{width: 100%; padding: 0 30rpx; box-sizing: border-box; overflow-x: scroll;overflow-y: hidden;text-align:center;background:#fff;}
.tabScroll{width:100%;position: relative;height: 70rpx;white-space:nowrap;margin:0 auto;text-align:left;background:#fff;}
.tabScroll view{display: inline;margin: 6rpx 15rpx;font-size: 30rpx;color: #999; text-align: center;}
.tabScroll view.active{color: #000;border-bottom: 8rpx solid #FFD71C;background:#fff;}
.tabAll,.tabOpen{font-size: 32rpx;margin-top: 70rpx; }
.tabOpen{position:absolute;z-index:99;bottom:5%;left:45%;}
.tagListAbsolute{position: absolute;z-index: 9;background: #fff;}
.movieList{margin:0 auto;padding: 0rpx 20rpx 178rpx 20rpx; box-sizing: border-box; margin-top: 120rpx}
.tagList{position:fixed;top:0;width: 100%;}
.tagAll{position:absolute;z-index:9;overflow-y:scroll;background:#fff;width: 100%;border-bottom: 1rpx solid #ccc;padding-bottom:60rpx;}
/* radio{opacity: 0}
radio:checked{color:red;} */

label.active{color: red;}
.tabAll{position: fixed;top:-1%;width:100%;display: block;background:#fff;}
.click{font-size: 28rpx; height: 40rpx; line-height: 40rpx }
.nomv{margin: 60% auto;}
.movieImg{
  border-radius: 12rpx;
  width: 210rpx;
  height: 294rpx;
  }
.item{
  margin-bottom: 30rpx
}
.icon_img{
  width: 40rpx;
  height: 40rpx;
  display: inline-block;
  vertical-align: middle;
  margin-top: -10rpx
}
.indexFixed{
  position: fixed;
  top:0;
  left:0;
  bottom:0;
  right:0;
}
