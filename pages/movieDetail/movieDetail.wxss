.header {
  width: 100%;
  margin: 0 auto;
  background: #222222;
  padding: 40rpx 0;
}
.button-hover,.other-button-hover {
  display: none;
}
.movieDetail {
  position: relative;
  margin: 0 40rpx;
  height: 302rpx; 
}

.movieDetail .poster {
  width: 210rpx;
  height: 294rpx;
  overflow: hidden;
  border-radius: 8px;
  background:#ECECEC;
}

.vip {
  position: absolute;
  /* left: 168rpx; */
  top: 0;
  width:48rpx;
  height:36rpx;
  line-height: 36rpx;
  background:rgba(255,98,70,1); 
  font-size: 20rpx;
  font-weight:500;
  font-family:PingFangSC-Medium,PingFangSC;
  color:#ffffff;
  text-align: center;
}

.main-vip {
  width:52rpx;
  height:36rpx;
  line-height: 36rpx;
  border-radius: 8px;
  left: 158rpx;
}

.movieDetail .describe {
  position: absolute;
  top: 0;
  left: 250rpx;
  width: 414rpx;
}

.movieDetail .describe .title {
  width: 420rpx;
  font-size: 44rpx;
  line-height: 60rpx;
  color: #fff;
  font-weight: bold;
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.movieDetail .tags {
  font-size: 24rpx;
  height: 34rpx;
  line-height: 34rpx;
  color: #999999;
  margin-top: 10rpx;
}

.movieDetail .like {
  position: absolute;
  left: 250rpx;
  top: 210rpx;
  width: 84rpx;
  height: 84rpx;
}

.like image {
  width: 100%;
  height: 100%;
}

.movieDetail .movie-push {
  position: absolute;
  top: 210rpx;
  left: 354rpx;
  width: 316rpx;
  height: 84rpx;
}

.movie-push image {
  width: 100%;
  height: 100%;
}

.push-area {
  width: auto;
  overflow-x: scroll;
  overflow-y: hidden;
  white-space : nowrap;
}

.dsj-item {
  display: inline-block;
  width:108rpx;
  height:108rpx;
  background:rgba(51,51,51,1);
  border-radius:8px;
  text-align: center;
  line-height: 108rpx;
  font-size:32rpx;
  font-weight:bold;
  color:rgba(255,255,255,1);
  margin-left: 20rpx;
}

.push-area .dsj-item:nth-child(1) {
  margin-left: 40rpx;
}
.push-area .dsj-item:last-child {
  margin-right: 40rpx;
}

.dsj-active {
  background:rgba(255,215,28,1);
  color: #000;
}

.zy-item {
  position: relative;
  display: inline-block;
  width: 280rpx;
  height: 132rpx;
