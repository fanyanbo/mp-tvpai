/* pages/edit/edit.wxss */
page{
  background-color: rgb(248, 248, 248)
}
.input-area{
  height: 110rpx;
  line-height: 110rpx;
  font-size:34rpx;
  background: #fff;
  margin-top: 30rpx;
  margin-bottom: 20rpx;
  padding:0 30rpx;
}
.editInput{
  height: 110rpx;
  line-height: 110rpx;
  padding-left: 20rpx;

}
.accountLog{
  width: 90%;
  margin: 0 auto;
  background: #FF6686;
  height: 94rpx;
  text-align: center;
  line-height: 94rpx;
  border-radius: 16rpx;
  color: #fff;
  font-size: 36rpx;
  margin-top: 60rpx
}
.edit{
  float: right;
  color:#888;
}
.arrow{
  height: 30rpx;
  width: 18rpx;
  vertical-align: middle;
  display: inline-block
}
.maskLayer{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.35;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.choose{
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  z-index: 9999;
  background: #f9f9f9;
}
.ask{
  height: 130rpx;
  line-height: 130rpx;
  color: #888;
  font-size: 28rpx;
  text-align: center;
  border-bottom:solid 1px #ece1e1
}
.sure{
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color:#E64340;
  font-size: 36rpx
}
.clearBtn{
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color:#000;
  font-size: 36rpx;
  border-top: #f1f1f1 solid 15rpx;
}
.indexFixed{
  position: fixed;
  top:0;
  left:0;
  bottom:0;
  right:0;
}