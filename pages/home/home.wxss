page{
  background-color: rgb(248, 248, 248)
}
.none{
  display: none;
}
.block{
  display: block;
}
.container{
  font-size: 0;
  padding-bottom: 100rpx;
}
.mydevices{
  height: 90rpx;
  line-height: 90rpx;
  position: relative;
  width: 90%;
  margin: 0 auto;
  font-size: 28rpx;
  color: #999;
}
.no-devices{
  width: 100%;
}
.devices{
  background: #fff;
  width: 670rpx;
  height: 218rpx;
  color: #444;
  margin: 0 auto;
  border-radius: 15rpx;
  margin-bottom: 30rpx
}
.device-top{
  display: flex;
  align-items: center;
  width: 510rpx;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
  font-size: 36rpx;
  color: #444;
}
.device-name{
  width: 350rpx;
}
.edit{
  border-radius: 20rpx;
  background: #ECECEC;
  color:#999;
  font-size:26rpx;
  width: 80rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  margin-left: 50rpx;
}
.device-top-left{
  width: 510rpx;
  float:left;
}
.device-top-right{
  float:left;
  height: 218rpx;
  line-height:218rpx;
  width: 160rpx;
  text-align: center;
  font-size: 34rpx;
  color:#999;
  border-left:solid 1px #ECECEC ;
  box-sizing: border-box;  
}
.device-model{
  width: 160rpx;
  height: 218rpx;
}
.inaline{
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis
}
.device-content{
  width: 100%;
  height: 34rpx;
  line-height: 34rpx;
  padding:0 20rpx;
  box-sizing: border-box;
  display: flex;
  font-size: 24rpx;
  color: #999;
  justify-content: space-between;
}
.more{
  margin-left: 20rpx;
  width: 20rpx;
  height: 20rpx;
  vertical-align:middle;
  transform: rotate(90deg);
}
.moretop{
  transform: rotate(-90deg)
}
.name-more{
  margin-left: 20rpx;
  width: 20rpx;
  height: 20rpx;  
  margin-bottom:-4rpx;
}
.morebind{
  margin-top: 20rpx;
  display: flex;
  justify-content: flex-end;
  padding-right: 20rpx;
}
.morebind text{
  font-size: 22rpx;
  color: #0096e3;
  text-align: right;
}
.scan{
  width: 60%;
  margin-top: 50px
}
.binding{
  width: 90%;
  height: 94rpx;
  line-height:94rpx;
  background: #fff;
  color: #000;
  margin: 15rpx auto;
  font-size: 36rpx;
  text-align: center;
  border-radius: 15rpx
}
.swiper-box{ display: block; height: 100%; width: 100%;overflow-y:scroll; margin-bottom: 300rpx}  
.swiper-box view{  
text-align: center;  
}  
.swiper_list .image1,.swiper_list .image2{
  width: 750rpx;
  height: 1148rpx
}
.swiper_list .image3{
  width: 750rpx;
  height: 1240rpx
}
.swiper_list .image4{
  width: 750rpx;
  height: 2114rpx
}
.swiper_list image{
  display: inline-block;
  float: left
}
._binding{
  width: 90%;
  margin: 0 auto;
  margin-bottom: 80rpx;
  background: #FFD71C;
  color: #333
}