input{width: 94%;margin: 0 auto;height: 75rpx;line-height: 75rpx;background: #eeeeee;font-size: 30rpx;color:#989A9C;display: block;text-indent:80rpx;}
.searchKey{position: fixed;top: 0;background: #fff;z-index: 9;height:100%;width: 100%;}
.searchKey .title{font-size:36rpx;margin-left: 3%;margin-bottom: 5%;display: inline-block;width: 82%;}
.searchKey .close{font-size: 36rpx;display: inline-block;text-align:right;width: 12%;}
.search{width:20px;height:20px;position:absolute;top:17rpx;left:6%;z-index:9;}
.search-body{width: 94%;margin: 5% auto;}
.search-body title{font-size: 28rpx;}
.search-body .list{margin-top: 8%;font-size:25rpx;}
.search-body .list ul li{display: inline-block; padding: 8rpx 16rpx;border: 1rpx solid #eeeeee;color: #353535;background: #eeeeee;border-radius: 5rpx;margin:1.5%;
}

.searchList{margin-top: 50rpx;}
.searchList navigator{display: inline-block;}
.searchList view{width: 25%;height: 160rpx;display: inline-block;text-align: center;vertical-align: top;}
.searchList view image{width:65rpx;height: 65rpx;display: block;margin: 0 auto;}
.searchList view li{width: 60rpx;height: 40rpx;line-height: 40rpx;font-size: 24rpx;}

.movieList{overflow-y:scroll;height:90%;}
.movieList .item{margin-top: 5%;}
