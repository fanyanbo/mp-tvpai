.tab-bar {
  position: fixed;
  top: 68px;
  left: 0;
  right: 0;
  height: 48px;
  background: #fff;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
  z-index: 9999;
  border:2px solid blue;
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}


.tab-bar-item cover-view {
  font-size: 12px;
}
